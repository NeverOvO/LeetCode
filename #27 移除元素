class Solution {
    func removeElement(_ nums: inout [Int], _ val: Int) -> Int {
        var aa :[Int] = []
        for var i in 0..<nums.count {
            if(nums[i] != val){
                aa.append(nums[i])
            }
            i += 1
        }
    nums = aa;
    return nums.count
    }
}

//执行用时 :20 ms, 在所有 Swift 提交中击败了17.13%的用户
//内存消耗 :21 MB, 在所有 Swift 提交中击败了100.00%的用户
